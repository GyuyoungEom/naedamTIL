<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
       <setting name="jdbcTypeForNull" value="NULL" />      
  </settings>  
  <typeAliases>
  	<typeAlias type="vo.BoardVO" alias="BoardVO"/>
  	<typeAlias type="vo.AppUserVO" alias="AppUserVO"/>
  	<typeAlias type="vo.TodoVO" alias="TodoVO"/>
  	<typeAlias type="vo.TodoVO2" alias="TodoVO2"/>
  </typeAliases>
  <mappers>
  	<mapper resource="mybatis/mapper/BoardMapper.xml"/>
  	<mapper resource="mybatis/mapper/AppUserMapper.xml"/>
  	<mapper resource="mybatis/mapper/TodoMapper.xml"/>
  </mappers>
</configuration>